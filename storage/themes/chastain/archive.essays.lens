<koken:include file="inc/header.html" />

<div id="text">

	<koken:load limit="20">

		<div class="head">

			<koken:link to="essays">{{ labels.essay.plural case="lower" }}</koken:link>&nbsp;&nbsp;/&nbsp;&nbsp;{{ archive.type.plural case="lower" }}&nbsp;&nbsp;/&nbsp;&nbsp;{{ archive.title }}

		</div>

		<koken:loop>

			<koken:include file="inc/article.html" />

		</koken:loop>

		<koken:else>

			No essays found.

	</koken:load>

</div>

<koken:include file="inc/footer.html" />